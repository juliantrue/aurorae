{
  "name": "@aurorae/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./prisma/*": "./prisma/*"
  },
  "scripts": {
    "prisma": "DATABASE_URL=file:./aurorae.db prisma",
    "prisma:generate": "DATABASE_URL=file:./aurorae.db prisma generate",
    "prisma:migrate": "DATABASE_URL=file:./aurorae.db prisma migrate dev",
    "prisma:db-push": "DATABASE_URL=file:./aurorae.db prisma db push",
    "prisma:studio": "DATABASE_URL=file:./aurorae.db prisma studio",
    "seed": "DATABASE_URL=file:./aurorae.db tsx scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0"
  },
  "devDependencies": {
    "prisma": "^6.17.0",
    "csv-parse": "^6.1.0"
  }
}
